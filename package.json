{
  "name": "infraquery",
  "version": "1.0.0",
  "description": "Open-source, Agentless network discovery and data extraction engine. Discovers, fingerprints, and maps IT, IoT, and OT devices via SNMP, Modbus, ONVIF, WMI, Prometheus, BACnet, HTTP, and more",
  "main": "dist/index.js",
  "bin": "./dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "yarn build",
    "release": "bash scripts/release.sh"
  },
  "keywords": [
    "network",
    "scanner",
    "port-scan",
    "tcp",
    "udp",
    "ping",
    "icmp",
    "security",
    "snmp",
    "modbus",
    "onvif",
    "wmi",
    "prometheus",
    "bacnet",
    "infrastructure",
    "discovery",
    "iot",
    "ot"
  ],
  "author": "Maprix",
  "license": "MPL-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maprixcom/InfraQuery.git"
  },
  "bugs": {
    "url": "https://github.com/maprixcom/InfraQuery/issues"
  },
  "homepage": "https://github.com/maprixcom/InfraQuery#readme",
  "devDependencies": {
    "@types/dns-packet": "^5",
    "@types/node": "^20.10.0",
    "@types/xml2js": "^0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "asn1.js": "^5.4.1",
    "axios": "^1.12.2",
    "bacstack": "^0.0.1-beta.14",
    "bplist-parser": "^0.3.2",
    "dns-packet": "^5.6.1",
    "net-snmp": "^3.26.0",
    "node-opcua": "^2.157.0",
    "ntlm-parser": "^1.0.9",
    "soap": "^1.5.0",
    "st-ethernet-ip": "^2.7.5",
    "uuid": "^13.0.0",
    "windows-release": "^7.0.0",
    "xml2js": "^0.6.2"
  }
}
