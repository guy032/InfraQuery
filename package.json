{
  "name": "infraquery",
  "version": "1.0.0",
  "description": "Network scanner with TCP/UDP port scanning and ping sweep capabilities",
  "main": "dist/index.js",
  "bin": "./dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "prepublish": "yarn build"
  },
  "keywords": [
    "network",
    "scanner",
    "port-scan",
    "tcp",
    "udp",
    "ping",
    "icmp",
    "security"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/dns-packet": "^5",
    "@types/node": "^20.10.0",
    "@types/xml2js": "^0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "asn1.js": "^5.4.1",
    "axios": "^1.12.2",
    "bacstack": "^0.0.1-beta.14",
    "bplist-parser": "^0.3.2",
    "dns-packet": "^5.6.1",
    "net-snmp": "^3.26.0",
    "node-opcua": "^2.157.0",
    "ntlm-parser": "^1.0.9",
    "soap": "^1.5.0",
    "st-ethernet-ip": "^2.7.5",
    "uuid": "^13.0.0",
    "windows-release": "^7.0.0",
    "xml2js": "^0.6.2"
  }
}
