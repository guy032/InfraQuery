# Track binary executables with Git LFS
bin/mac/cloudquery filter=lfs diff=lfs merge=lfs -text
bin/mac/naabu filter=lfs diff=lfs merge=lfs -text
bin/mac/nping filter=lfs diff=lfs merge=lfs -text
bin/mac/udpx filter=lfs diff=lfs merge=lfs -text
bin/mac/udpz filter=lfs diff=lfs merge=lfs -text

# Track plugin files with Git LFS
bin/mac/.cq/plugins/**/*.plugin filter=lfs diff=lfs merge=lfs -text
bin/mac/.cq/plugins/**/plugin filter=lfs diff=lfs merge=lfs -text
# Track zip files with Git LFS
*.zip filter=lfs diff=lfs merge=lfs -text
# Track other common binary formats
*.exe filter=lfs diff=lfs merge=lfs -text
*.dll filter=lfs diff=lfs merge=lfs -text
*.so filter=lfs diff=lfs merge=lfs -text
*.dylib filter=lfs diff=lfs merge=lfs -text
*.tar filter=lfs diff=lfs merge=lfs -text
*.tar.gz filter=lfs diff=lfs merge=lfs -text
*.tgz filter=lfs diff=lfs merge=lfs -text
*.gz filter=lfs diff=lfs merge=lfs -text
*.7z filter=lfs diff=lfs merge=lfs -text
*.rar filter=lfs diff=lfs merge=lfs -text
# Track binary data files
*.bin filter=lfs diff=lfs merge=lfs -text
*.dat filter=lfs diff=lfs merge=lfs -text
*.plugin filter=lfs diff=lfs merge=lfs -text
